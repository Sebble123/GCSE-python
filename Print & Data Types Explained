print("seb") #This is a string
print(4) #This is an integer
print(4.4) #This is a real/float
print("^") #This is a character
print(True) #This is a boolean

#Data Types
#CRIBS
#C = Character e.g. "a"
#R = Real/Float e.g. 4.66
#I = Interger e.g. 4
#B = Boolean e.g. True
#S = String e.g. "hello"print("seb") #This is a string
